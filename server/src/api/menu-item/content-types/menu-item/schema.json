{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 255,
      "minLength": 1
    },
    "description": {
      "type": "text",
      "maxLength": 255,
      "minLength": 1
    },
    "price": {
      "type": "decimal",
      "max": 9999,
      "min": 0,
      "default": 0
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "itemtype": {
      "type": "enumeration",
      "enum": [
        "starter",
        "lunch",
        "dessert"
      ]
    }
  }
}
